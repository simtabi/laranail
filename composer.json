{
    "name"              : "simtabi/laranail",
    "license"           : "MIT",
    "description"       : "Utility functions and helper methods",
    "type"              : "library",
    "keywords": [
        "helpers",
        "functions"
    ],
    "authors": [
        {
            "name": "Imani Manyara",
