            "aliases": {
                "SortItemsWithChildrenHelper"    : "Simtabi\\Laranail\\Facades\\SortItemsWithChildrenHelperFacade",
                "LanguagesFacade"                : "Simtabi\\Laranail\\Facades\\LanguagesFacade",
                "LaravelTools"                   : "Simtabi\\Laranail\\Facades\\LaravelToolsFacade",
                "Phegs"                          : "Simtabi\\Laranail\\Facades\\PhegsFacade"
            }
        }
    }
}




