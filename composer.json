    "require": {
        "jasny/dotkey": "dev-master",
        "simtabi/phegs": "dev-master",
        "ichtrojan/laravel-otp": "dev-master",
        "dyrynda/laravel-model-uuid": "^6.4.0",
        "ramsey/uuid": "^4.1.1",
        "dyrynda/laravel-efficient-uuid": "^4.3.0",
        "spomky-labs/otphp": "^v10.0.1",
        "nesbot/carbon": "^2.41",
        "intervention/validation": "^2.6.6",
        "cocur/slugify": "^4.0",
        "mccool/laravel-auto-presenter": "^7.5",
        "ausi/slug-generator": "^v1.1.1",
        "vincoweb/easy-php-file-info": "^v1.0.6",
        "respect/validation": "^2.2.1",
        "spatie/url": "^1.3.5",
        "bjeavons/zxcvbn-php": "^1.2.0",
        "giggsey/libphonenumber-for-php": "^8.12.18",
        "simtabi/memory-size": "dev-master",
        "mattkingshott/axiom": "^3.1.1",
        "propaganistas/laravel-phone": "^4.2",
        "laravel-validation-rules/phone": "^1.6",
        "irfa/php-sn-generator": "dev-master",
        "html2text/html2text": "^4.2",
        "gpslab/base64uid": "dev-master",
        "andrey-helldar/pretty-routes": "dev-main"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "Simtabi\\Laranail\\": "src/"
